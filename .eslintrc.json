{
  "env": {
    "shared-node-browser": true,
    "es2022": true
  },
  "extends": [
    "prettier",
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": "node"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ],
    "prettier/prettier": "warn",
    "arrow-body-style": "warn",
    "no-unused-vars": "warn",
    "no-useless-constructor": "warn",
    "no-console": "off"
  }
}
